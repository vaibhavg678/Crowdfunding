{
  "name": "Executive Light Professional",
  "background": "#F3F4F6",
  "foreground": "#1F2937",
  "tableAccent": "#2563EB",

  "dataColors": [
    "#2563EB",
    "#1E40AF",
    "#3B82F6",
    "#F59E0B",
    "#9CA3AF",
    "#0EA5E9"
  ],

  "visualStyles": {
    "*": {
      "*": {
        "title": [
          {
            "show": true,
            "fontFamily": "Segoe UI Semibold",
            "fontSize": 14,
            "color": { "solid": { "color": "#111827" } }
          }
        ],
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFFFFF" } },
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": true,
            "color": { "solid": { "color": "#E5E7EB" } }
          }
        ],
        "labels": [
          {
            "color": { "solid": { "color": "#374151" } },
            "fontSize": 11
          }
        ]
      }
    },

    "card": {
      "*": {
        "labels": [
          {
            "fontSize": 30,
            "color": { "solid": { "color": "#111827" } }
          }
        ],
        "categoryLabels": [
          {
            "color": { "solid": { "color": "#6B7280" } }
          }
        ]
      }
    },

    "slicer": {
      "*": {
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFFFFF" } }
          }
        ],
        "items": [
          {
            "color": { "solid": { "color": "#374151" } }
          }
        ],
        "outlineColor": [
          {
            "color": { "solid": { "color": "#E5E7EB" } }
          }
        ]
      }
    }
  }
}